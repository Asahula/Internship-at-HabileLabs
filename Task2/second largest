def duplicates(nums):
    nums.sort()
    i=0
    while i < len(nums)-1:
        if nums[i]==nums[i+1]:
            nums.pop(i)
        else:
            i = i+1
    return(nums)
def SecondLargest(l):
    m = duplicates(l)
    return(m[-2])
try:
    l = list(map(int, input("Enter a list: ").split()))
    print(SecondLargest(l))
except ValueError:
    print("Enter an integer in numerical form")