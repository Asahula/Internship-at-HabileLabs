def fizz_buzz():
    try:
        for i in range(1, 51):
            if i % 3 == 0 and i % 5 == 0:
                print("FizzBuzz")
            elif i % 3 == 0:
                print("Fizz")
            elif i % 5 == 0:
                print("Buzz")
            else:
                print(i)
    except Exception as e:
        print(f"An unexpected error occurred: {e}")


if __name__ == "__main__":
    fizz_buzz()
